class Solution {
public:

    int findDuplicate(vector<int>& arr) {
        
        int n = arr.size();
        int ans = 0;

       unordered_map<int,int> freq;

       for(int i=0;i<n;i++){
           freq[arr[i]]++;
       }

       for(int i=0;i<n;i++){
           if(freq[arr[i]] > 1)
            {
                ans = arr[i];
                break;
            }
       }

       return ans;

    }
};
