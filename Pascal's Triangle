#include <bits/stdc++.h>
using namespace std;

vector<int> generateRow(int row){
  long long ans = 1;
  vector<int> RowAns;
  RowAns.push_back(1);
  for(int col=1;col<row;col++){
    ans = ans*(row-col);
    ans = ans/(col);
    RowAns.push_back(ans); 
  }

  return RowAns;
}

vector<vector<int>> printPascal(int n){
  vector<vector<int>> ans;

  for(int i=1;i<=n;i++){
    ans.push_back(generateRow(i));
  }
  
  return ans;
}

